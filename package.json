  "description": "Our concierge nurses provide comprehensive, bespoke care, from post-surgical recovery and chronic condition management to wellness optimization and palliative support. With 24/7 coordination and clinical oversight, we ensure your health is managed with precision and compassion, wherever you are.",
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@lhci/cli": "0.15.1",
    "lighthouse": "13.0.1",
    "web-vitals": "5.1.0"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "homepage": "https://github.com/dylarcher/legacyconcierge#readme",
  "keywords": [
    "healthcare",
    "nursing",
    "in-home-care",
    "end-of-life-care",
    "static-site",
    "i18n",
    "accessibility",
    "wcag"
  ],
  "license": "ISC",
  "main": "index.html",
  "name": "legacy-concierge",
  "optionalDependencies": {
    "@biomejs/cli-darwin-arm64": "2.2.7",
    "@biomejs/cli-darwin-x64": "2.2.7",
    "@biomejs/cli-linux-arm64": "2.2.7",
    "@biomejs/cli-linux-arm64-musl": "2.2.7",
    "@biomejs/cli-linux-x64": "2.2.7",
    "@biomejs/cli-linux-x64-musl": "2.2.7",
    "@biomejs/cli-win32-arm64": "2.2.7",
    "@biomejs/cli-win32-x64": "2.2.7",
    "bufferutil": "^4.0.8",
    "encoding": "^0.1.13",
    "utf-8-validate": "^6.0.4"
  },
  "overrides": {
    "glob": "^11.0.0",
    "inflight": "npm:@badgateway/inflight@^1.0.0",
    "rimraf": "^6.0.1",
    "tmp": "^0.2.4",
    "ws": {
      ".": "^8.18.0",
      "bufferutil": "^4.0.8",
      "utf-8-validate": "^6.0.4"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylarcher/legacyconcierge.git"
  },
  "scripts": {
    "biome": "npx @biomejs/biome",
    "check": "npm run biome -- check .",
    "check:fix": "npm run biome -- check --write .",
    "dev": "python3 -m http.server 8000",
    "lighthouse": "npx lhci autorun",
    "lighthouse:mobile": "npm run lighthouse --config=configs/lighthouserc.mobile.js",
    "lighthouse:quick": "npx lighthouse http://localhost:8000 --view --preset=desktop",
    "lighthouse:report": "npm run lighthouse --upload.target=filesystem --upload.outputDir=.lighthouse",
    "lint": "npm run biome -- lint .",
    "lint:fix": "npm run biome -- lint --write .",
    "mdlint": "npx markdownlint-cli **/*.md",
    "mdlint:fix": "npm run mdlint --fix",
    "paths:audit": "npm run paths:fix --dry-run",
    "paths:fix": "node bin/rewrite-paths.cjs",
    "test": "echo 'Error: no test suite configured' && exit 1",
    "tidy": "npm run biome -- format .",
    "tidy:fix": "npm run biome -- format --write .",
    "validate": "npm run check && npm run paths"
  },
  "type": "module",
  "version": "1.0.0"
}
